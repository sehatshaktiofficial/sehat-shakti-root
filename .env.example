# SehatShakti Frontend Environment Configuration

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_APP_NAME=SehatShakti
EXPO_PUBLIC_APP_VERSION=2.0.0

# App Configuration
EXPO_PUBLIC_APP_SCHEME=sehatshakti
EXPO_PUBLIC_APP_SLUG=sehat-shakti

# Development Configuration
EXPO_PUBLIC_DEBUG=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Feature Flags
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_AI_SYMPTOM_CHECKER=true
EXPO_PUBLIC_ENABLE_VIDEO_CONSULTATION=true
EXPO_PUBLIC_ENABLE_MEDICINE_DATABASE=true

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here
