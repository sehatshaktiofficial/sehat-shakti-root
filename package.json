{
  "name": "sehat-shakti-app",
  "version": "2.0.0",
  "description": "Unified Telemedicine Platform - SehatShakti",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "install:all": "npm install && cd backend && npm install",
    "dev": "concurrently \"npm run backend:dev\" \"npm start\"",
    "setup": "node setup.js",
    "build": "expo build",
    "test": "jest",
    "test:backend": "cd backend && npm test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "clean": "expo r -c",
    "reset": "expo r -c && npm install && cd backend && npm install"
  },
  "keywords": [
    "telemedicine",
    "healthcare",
    "rural-health",
    "offline-first",
    "react-native",
    "expo"
  ],
  "author": "SehatShakti Team",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@nozbe/watermelondb": "^0.28.0",
    "@nozbe/with-observables": "^1.6.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-react-native": "^1.0.0",
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "expo": "~54.0.6",
    "expo-crypto": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "18.3.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.44.0",
    "eslint-config-react-native": "^4.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.6.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
